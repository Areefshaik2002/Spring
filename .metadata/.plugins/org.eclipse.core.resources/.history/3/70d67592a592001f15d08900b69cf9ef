package com.spring;

public class Developer {
	private Laptop laptop;	
	
	public Developer(Laptop laptop) {
		super();
		this.laptop = laptop;
	}
	
	public Laptop getLaptop() {
		return laptop;
	}

	public void setLaptop(Laptop laptop) {
		this.laptop = laptop;
	}

	public void startCoding() {
		laptop.codeInLaptop();
	}
}
